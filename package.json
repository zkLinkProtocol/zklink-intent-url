{
  "name": "intenturl",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "validate": "ts-node scripts/check-actions.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/$npm_config_name -- -d ./src/typeorm.config.ts",
    "migration:create": "npm run typeorm migration:create ./src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run -- -d ./src/typeorm.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@turnkey/sdk-server": "^1.3.0",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "aws-sdk": "^2.1659.0",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "ethers": "^6.13.2",
    "html-to-md": "^0.8.6",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.0",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.10.0",
    "nestjs-cls": "^4.4.1",
    "node-telegram-bot-api": "^0.66.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "{src,libs,test}/**/*.ts": [
      "eslint --quiet --fix"
    ],
    "{src,libs,test}/**/*.{json,ts}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@action/novaswap(|/.*)$": "<rootDir>/libs/novaswap/src/$1",
      "^@core/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@action/buy-me-a-coffee(|/.*)$": "<rootDir>/libs/buy-me-a-coffee/src/$1",
      "^@action/red-envelope(|/.*)$": "<rootDir>/libs/buy-me-a-coffee/src/$1",
      "^@action/split-bill(|/.*)$": "<rootDir>/libs/split-bill/src/$1",
      "^@action/registry(|/.*)$": "<rootDir>/libs/registry/src/$1",
      "^@action/pump-fun(|/.*)$": "<rootDir>/libs/pump-fun/src/$1",
      "^@action/news(|/.*)$": "<rootDir>/libs/news/src/$1",
      "^@action/buy-nft-magic-eden(|/.*)$": "<rootDir>/libs/buy-nft-magic-eden/src/$1",
      "^@action/buy-nft-okx(|/.*)$": "<rootDir>/libs/buy-nft-okx/src/$1",
      "^@action/mint-nft(|/.*)$": "<rootDir>/libs/mint-nft/src/$1",
      "^@action/mint-nova-nft(|/.*)$": "<rootDir>/libs/mint-nova-nft/src/$1",
      "^@core/shared(|/.*)$": "<rootDir>/libs/data/src/$1",
      "^@action/shared-red-packet(|/.*)$": "<rootDir>/libs/shared-red-packet/src/$1",
      "^@action/pre-sale(|/.*)$": "<rootDir>/libs/pre-sale/src/$1",
      "^@action/magic-swap(|/.*)$": "<rootDir>/libs/magic-swap/src/$1",
      "^@action/agx(|/.*)$": "<rootDir>/libs/agx/src/$1"
    }
  }
}
