{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@types/*": [
        "types/*"
      ],
      "@core/utils": [
        "libs/utils/src"
      ],
      "@core/utils/*": [
        "libs/utils/src/*"
      ],
      "@action/novaswap": [
        "libs/novaswap/src"
      ],
      "@action/novaswap/*": [
        "libs/novaswap/src/*"
      ],
      "@action/buy-me-a-coffee": [
        "libs/buy-me-a-coffee/src"
      ],
      "@action/buy-me-a-coffee/*": [
        "libs/buy-me-a-coffee/src/*"
      ],
      "@action/cross-chain-swap": [
        "libs/cross-chain-swap/src"
      ],
      "@action/cross-chain-swap/*": [
        "libs/cross-chain-swap/src/*"
      ],
      "@action/red-envelope": [
        "libs/red-envelope/src"
      ],
      "@action/red-envelope/*": [
        "libs/red-envelope/src/*"
      ],
      "@action/slipt-bill": [
        "libs/split-bill/src"
      ],
      "@action/slipt-bill/*": [
        "libs/split-bill/src/*"
      ],
      "@action/news": [
        "libs/news/src"
      ],
      "@action/news/*": [
        "libs/news/src/*"
      ],
      "@action/registry": [
        "libs/registry/src",
        "libs/registry/src/index.ts"
      ],
      "@action/registry/*": [
        "libs/registry/src/*"
      ],
      "@action/pump-fun": [
        "libs/pump-fun/src"
      ],
      "@action/pump-fun/*": [
        "libs/pump-fun/src/*"
      ],
      "@action/buy-nft": [
        "libs/buy-nft/src"
      ],
      "@action/buy-nft/*": [
        "libs/buy-nft/src/*"
      ],
      "@action/buy-nft-okx": [
        "libs/buy-nft-okx/src"
      ],
      "@action/buy-nft-okx/*": [
        "libs/buy-nft-okx/src/*"
      ],
      "@action/mint-nft": [
        "libs/mint-nft/src"
      ],
      "@action/mint-nft/*": [
        "libs/mint-nft/src/*"
      ],
      "@action/mint-nova-nft": [
        "libs/mint-nova-nft/src"
      ],
      "@action/mint-nova-nft/*": [
        "libs/mint-nova-nft/src/*"
      ],
      "@core/shared": [
        "libs/shared/src"
      ],
      "@core/shared/*": [
        "libs/shared/src/*"
      ],
      "@action/shared-red-packet": [
        "libs/shared-red-packet/src"
      ],
      "@action/shared-red-packet/*": [
        "libs/shared-red-packet/src/*"
      ],
      "@action/pre-sale": [
        "libs/pre-sale/src"
      ],
      "@action/pre-sale/*": [
        "libs/pre-sale/src/*"
      ],
      "@action/magic-swap": [
        "libs/magic-swap/src"
      ],
      "@action/magic-swap/*": [
        "libs/magic-swap/src/*"
      ]
    }
  }
}